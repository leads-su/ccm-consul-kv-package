{
    "name": "ccm/consul-kv",
    "description": "CCM package which provides integration with Consul KV storage",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "ccm",
        "leads",
        "consul",
        "agent"
    ],
    "homepage": "https://gitlab.leads.local/ccm/ccm-consul-kv-package",
    "authors": [
        {
            "name": "Zhivolupov Ivan",
            "email": "iz@leads.su",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ccm/domain": "^1.0",
        "ccm/users": "^1.0",
        "darki73/consul-laravel-sdk": "^1.0",
        "spatie/laravel-event-sourcing": "^5.0"
    },
    "require-dev": {
        "ccm/testing": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ConsulConfigManager\\Consul\\KeyValue\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "ConsulConfigManager\\Consul\\KeyValue\\Test\\": "tests/"
        },
        "files": []
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConsulConfigManager\\Consul\\KeyValue\\Providers\\ConsulKeyValueServiceProvider"
            ]
        }
    },
    "scripts": {
        "psr": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "test": [
            "phpunit --color=always"
        ],
        "test:feature": [
            "phpunit --color=always --testsuite \"Consul Feature Test Suite\""
        ],
        "test:integration": [
            "phpunit --color=always --testsuite \"Consul Integration Test Suite\""
        ],
        "test:unit": [
            "phpunit --color=always --testsuite \"Consul Unit Test Suite\""
        ],
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "phpunit --color=always --coverage-clover=\"output/code-coverage/clover.xml\""
        ],
        "test:coverage-text": [
            "@putenv XDEBUG_MODE=coverage",
            "phpunit --color=always --coverage-text"
        ],
        "test:coverage-gitlab": [
            "@putenv XDEBUG_MODE=coverage",
            "phpunit --color=never --coverage-text"
        ],
        "test:coverage-html": [
            "@putenv XDEBUG_MODE=coverage",
            "phpunit --color=always --coverage-html=\"output/code-coverage\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false,
        "gitlab-domains": ["gitlab.leads.local"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "git@gitlab.leads.local:ccm/ccm-testing-package.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.leads.local:ccm/ccm-domain-package.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.leads.local:ccm/ccm-users-package.git"
        }
    ]
}
